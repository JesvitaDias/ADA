#include <stdio.h>
#include <string.h>

#define MAX_CHAR 256

int firstUniqChar(char* s) {
    int count[MAX_CHAR] = {0};  
    char queue[strlen(s)]; 
    int front = 0, rear = 0; 

    
    for (int i = 0; s[i] != '\0'; i++) {
        count[s[i]]++; 
        queue[rear++] = s[i];  
    }

    
    while (front < rear) {
        char c = queue[front++];  
        if (count[c] == 1) { 
            
            for (int i = 0; s[i] != '\0'; i++) {
                if (s[i] == c) {
                    return i;  
                }
            }
        }
    }

    return -1;  
}


